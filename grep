<project name="CDK" default="html" basedir=".">

    <property file="build.props"/>

    <!-- $Id$ -->

    <property name="vecmath.src" value="gcj/vecmath" />
    <property name="vecmath.javadoc.dir" value="doc/vecmath/api" />

